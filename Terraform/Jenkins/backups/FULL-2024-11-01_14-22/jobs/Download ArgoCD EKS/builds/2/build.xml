                sh &apos;&apos;&apos;
                chmod +x final-project-terraform/Jenkins/cluster-install-argocd.sh
                ./final-project-terraform/Jenkins/cluster-install-argocd.sh
                &apos;&apos;&apos;
            }
        }
    }

    post {
        success {
            echo &apos;Argo CD installation completed successfully!&apos;
